stingalleman.nl {
  tls {
    dns cloudflare {env.CF_API_TOKEN}
  }

  respond 200 {
    body "maintenance..."
  }
}

# CDN
cdn.stingalleman.nl {
  tls {
    dns cloudflare {env.CF_API_TOKEN}
  }

  root * /storage/cdn.stingalleman.nl
  file_server browse

  encode zstd gzip

  log {
    output file /logs/cdn.log
    format console
  }
}

zeus.stingalleman.nl {
  respond 200 {
    body "aa"
  }
}
